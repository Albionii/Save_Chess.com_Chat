{
  "manifest_version": 3,
  "name": "Chat saver",
  "version": "1.0",
  "description": "Save Chess.com chat.",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
    
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}